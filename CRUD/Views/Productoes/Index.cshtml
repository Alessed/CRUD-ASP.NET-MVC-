@model IEnumerable<CRUD.Models.Producto>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DblCantidad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DatFechaRegistro)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StrNombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DblPrecio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StrClave)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DblCantidad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DatFechaRegistro)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StrNombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DblPrecio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StrClave)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IntProducto">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IntProducto">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IntProducto">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>


